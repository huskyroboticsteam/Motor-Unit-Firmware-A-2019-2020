      <field name="RXGTE96" from="19" to="19" access="RW" resetVal="" desc="Rx Error Count is greater or equal to 96 Decimal" hidden="false" />
    </register>
    <register name="CAN_CAN_ECR" address="0x402F0018" bitWidth="32" desc="The Error Capture register operates in two different modes: &#xA; Free running mode - ECR displays the fiels and bit positions within the current CAN frame &#xA; Error Capture mode - ECR samples the field and bit position when a CAN error is detected &#xA; In order to sample such an event, the ECR needs to be armed by performing a write access to it. &#xA; When armed, the ECR only captures one error event.  &#xA; For successive error captures, the ECR needs to be armed again." hidden="false">
      <field name="ECR_STATUS" from="0" to="0" access="RW" resetVal="" desc="ECR STATUS: &#xA;         0: ECR register captured an error, or it is in free running mode &#xA;         1: ECR register is armed" hidden="false" />
      <field name="ERROR_TYPE" from="3" to="1" access="R" resetVal="" desc="Error type: &#xA;         000 : Arbitration loss &#xA;         001 : Bit Error &#xA;         010 : Bit Stuffing Error &#xA;         011 : Acknowledge Error &#xA;         100 : Form Error &#xA;         101 : CRC Error &#xA;         Others : N/A" hidden="false" />
      <field name="RX_MODE" from="4" to="4" access="R" resetVal="" desc="RX Mode: &#xA;         0: No status &#xA;         1: CAN Controller is receiver" hidden="false" />
      <field name="TX_MODE" from="5" to="5" access="R" resetVal="" desc="TX Mode: &#xA;         0: No status &#xA;         1: CAN Controller is transmitter" hidden="false" />
      <field name="BIT" from="11" to="6" access="R" resetVal="" desc="Bit number inside of Field" hidden="false" />
      <field name="Field" from="16" to="12" access="R" resetVal="" desc="Field: &#xA;         0x00 : Stopped &#xA;         0x01 : Synchronize &#xA;         0x05 : Interframe &#xA;         0x06 : Bus Idle &#xA;         0x07 : Start of Frame &#xA;         0x08 : Arbitration &#xA;         0x09 : Control &#xA;         0x0A : Data &#xA;         0x0B : CRC &#xA;         0x0C : ACK &#xA;         0x0D : End of frame &#xA;         0x10 : Error flag &#xA;         0x11 : Error echo &#xA;         0x12 : Error delimiter &#xA;         0x18 : Overload flag &#xA;         0x19 : Overload echo &#xA;         0x1A : Overload delimiter &#xA;         Others : N/A" hidden="false" />
    </register>
